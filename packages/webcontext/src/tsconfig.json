{
	"compilerOptions": {
		"pretty": true,
		"stripInternal": true,
		"experimentalDecorators": true,
		"strict": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noImplicitOverride": true,
		"noUncheckedIndexedAccess": true,
		"importHelpers": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"esModuleInterop": true,
		"target": "esnext",
		"lib": ["dom", "es2015"],
		"outDir": "../dist",
		"sourceMap": true
	}
}
