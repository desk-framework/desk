{
	"compilerOptions": {
		"pretty": true,
		"declaration": true,
		"stripInternal": true,
		"experimentalDecorators": true,
		"strict": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noImplicitOverride": true,
		"noUncheckedIndexedAccess": true,
		"importHelpers": true,
		"module": "esnext",
		"moduleResolution": "node",
		"target": "esnext",
		"lib": ["dom", "esnext"],
		"outDir": "../dist"
	}
}
