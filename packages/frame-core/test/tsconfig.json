{
	"compilerOptions": {
		"stripInternal": true,
		"useDefineForClassFields": true,
		"strict": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noImplicitOverride": true,
		"noUncheckedIndexedAccess": true,
		"importHelpers": true,
		"module": "esnext",
		"moduleResolution": "node",
		"target": "ES2022",
		"lib": ["dom", "esnext"],
		"jsx": "react",
		"jsxFactory": "ui.jsx",
		"outDir": "../.test-run",
		"sourceMap": true,
		"pretty": true
	},
	"files": ["index.ts"]
}
