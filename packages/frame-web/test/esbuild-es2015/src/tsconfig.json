{
	"compilerOptions": {
		"experimentalDecorators": true,
		"strict": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUncheckedIndexedAccess": true,
		"module": "esnext",
		"moduleResolution": "node",
		"target": "ES2015",
		"lib": ["dom", "ES2015"],
		"jsx": "react",
		"jsxFactory": "JSX",
		"outDir": "../dist"
	}
}
