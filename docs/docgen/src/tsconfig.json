{
	"compilerOptions": {
		"strict": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noImplicitOverride": true,
		"noUncheckedIndexedAccess": true,
		"module": "Node16",
		"moduleResolution": "Node16",
		"target": "ES2020",
		"lib": ["DOM", "ES2020"],
		"declaration": true,
		"sourceMap": true,
		"outDir": "../lib"
	}
}
