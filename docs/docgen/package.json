{
	"name": "@desk-framework/docgen",
	"private": true,
	"version": "0.0.0",
	"license": "MIT",
	"type": "module",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"clean": "rimraf lib",
		"prebuild": "npm run clean",
		"build": "tsc -p src",
		"check-format": "prettier --check \"src/**/*.{ts,tsx}\""
	},
	"prettier": {
		"useTabs": true,
		"trailingComma": "all",
		"endOfLine": "lf",
		"overrides": [
			{
				"files": "*.yaml",
				"options": {
					"useTabs": false,
					"tabWidth": 2
				}
			}
		]
	},
	"devDependencies": {
		"@types/js-yaml": "^4.0.6",
		"js-yaml": "^4.1.0",
		"html-entities": "^2.4.0"
	}
}
