{
	"compilerOptions": {
		"noEmit": true,
		"strict": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noImplicitOverride": true,
		"noUncheckedIndexedAccess": true,
		"jsx": "react",
		"jsxFactory": "ui.jsx",
		"module": "ES2020",
		"moduleResolution": "bundler",
		"target": "ES2020",
		"outDir": "../dist"
	}
}
